// Faça um programa que gere 10 arquivos com o nome “testeXX”, onde XX é ”01”, ...,
//”10” (exemplo: teste01.txt”). Cada arquivo deve conter dentro dele o texto ”Texto do
//arquivo [NÚMERO DO ARQUIVO]”.
#include <stdio.h>

int main() {
    int i;
    FILE*file;
    char filename[21];
    for (i=1;i<=10;i++){
        
        sprintf(filename,"teste%02d.txt",i);
        file = fopen(filename, "w");
        if (file==NULL)return 1;
        fprintf(file,"Texto do arquivo [%d]",i);
        fclose(file);
    }

    return 0;
}
