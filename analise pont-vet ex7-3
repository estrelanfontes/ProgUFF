#include <stdio.h>

int main(){
int *pti;
int veti[]={10,7,2,6,3};
pti = veti;
printf("%d\n",*pti);
printf("%d\n",*(pti+2));
printf("%d\n",pti[4]);
printf("%d\n",pti[1]); //retorna 7, e não 10. Letra d está errada
printf("%d\n",*(veti+3));
printf("%d",*(veti+1)); //*veti == veti[0] entao *(veti+2)==2, assim por diante
}
