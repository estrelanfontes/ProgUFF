#include <stdio.h>
#include <math.h>

float delta(float a, float b, float c){
int d = (b*b-4*a*c); 
return d;
}

int main(){

double resultado, resultado2;
float a,b,c,d;
printf("digite um valor para a,b,c para o polinomio:\n");
scanf("%f %f %f",&a,&b,&c);
d = delta(a,b,c);

if (d<0){
printf("Error");
}
else {
resultado = (-b+sqrt(d))/(2*a);
resultado2 = (-b-sqrt(d))/(2*a);
printf("%f\n",resultado);
printf("%f",resultado2);
}
}
}
