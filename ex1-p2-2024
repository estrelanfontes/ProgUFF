#include <stdio.h>
#include <stdlib.h>

typedef struct ponto{
    int x,y,z;
}Ponto;

void troca(Ponto*p1,Ponto*p2){
    Ponto temp = *p1;
    *p1=*p2;
    *p2= temp;
}

void imprime(Ponto ponto){
    printf("%d ",ponto.x);
    printf("%d ",ponto.y);
    printf("%d \n",ponto.z);
}

Ponto*novoPonto(int x,int y,int z){
    Ponto *novo = (Ponto*)malloc(sizeof(Ponto));
    novo->x = x;
    novo->y = y;
    novo->z = z;
    return novo;
}

int main(){
    
    Ponto p1={2,4,6},p2={5,7,9},*p3;
    troca(&p1,&p2);
    imprime(p1);
    imprime(p2);
    p3 = novoPonto(1,3,5);
    imprime(*p3);
}
