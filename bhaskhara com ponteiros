#include <stdio.h>
#include <math.h>

int delta(int x, int y, int z){
int result;
result = y*y +(-4*x)*z;
return result;
    
}

int conta(float *r1, float *r2, int d, int *a, int *b, int *c){
*r1 = (-*b + sqrt(d))/(2*(*a));
*r2 = (-*b -sqrt(d))/(2*(*a));
}

int main() {
int a,b,c,d;
a = 4;
b = 2;
c = 0;
float raiz1, raiz2;
d = delta(a,b,c);
if (d<0) printf("nao hÃ¡ raiz negativa");
conta(&raiz1,&raiz2,d,&a,&b,&c);
printf("%f",raiz1);
printf("\n%f",raiz2);

    return 0;
}

