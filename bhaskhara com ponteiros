#include <stdio.h>
#include <math.h>

float delta(float x, float y, float z){
float result;
result = y*y -4*x*z;
return result;
    
}

void conta(float *r1, float *r2, float d, float *a, float *b, float *c){
*r1 = (-*b + sqrt(d))/(2*(*a));
*r2 = (-*b -sqrt(d))/(2*(*a));
}

int main() {
float a,b,c,d;
a = 4;
b = 2;
c = 0;
float raiz1, raiz2;
d = delta(a,b,c);
if (d<0) printf("nao hÃ¡ raiz negativa");
else{
conta(&raiz1,&raiz2,d,&a,&b,&c);
printf("%f",raiz1);
printf("\n%f",raiz2);
}
    return 0;
}
