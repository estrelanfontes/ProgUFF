#include <stdio.h>
int divs(int n, int *max, int *min);
int main() {
int n, n1, n2, bol;
printf("digite um inteiro: ");
scanf("%d", &n);
bol = divs(n, &n1, &n2);
if (bol == 1){
    printf("o numero nao é primo, seguem os divisores: %d,%d",n1,n2);
}
else if (bol == 0) printf("o numero é primo");
}

int divs(int n, int *max, int *min){
int i, confere = 0; 
for (i=2; i<n; i++){
    if (n%i == 0){
    *min = i;
    *max = n/i;
    return 1;
    }
    }
return 0;
}
