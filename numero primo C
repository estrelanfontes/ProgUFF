#include <stdio.h>
char selectprimo(int number){
int resultado;
resultado = 0;
if (number>0 && number>=3){ 
for (int i=2; i<number; i++){
    if (number%i == 0){
        resultado = 1;
        break;}
    }}
else if (number<0){
    resultado = 1;
}
return resultado;
}

int main(){
int input;
printf("digite um numero e descubra se é primo\n");
scanf("%d", &input);
if (selectprimo(input)==1){
    printf("não é primo");
}
else {
    printf("é primo");
}

return 0;
}
