#include <stdio.h>
#include <string.h>
int main (void) {
char *frase = "Otimo teste";
char *p , misterio[80];
int i = 0;
int j = 0;
p = frase + strlen ( frase ) - 1;//p= frase + 10 = 'O' + 10 = 'e' ultimo caractere
while (*p != ' ') {
misterio[i] = *p; //misterio recebe caracteres nao vazios de frase e de tras pra frente
i ++; p --; //*misterio = etset, i = 5
puts (misterio);
}

misterio[i] = ' '; i ++; //*misterio = 'etset '
while ( frase[j] != ' ') {//*frase='Otimo teste'
misterio [i] = frase[j];
j ++; i ++;
puts (misterio);
}
misterio[i] = '\0';
puts (misterio);//misterio = "etset Otimo"
return 0;
}

//strlne() conta o tamanho da str desconsiderando o \0
//puts() printa a string
