#include <stdio.h>
#include <conio.h>
#define MAX 20

int main() {
    char c, vet[MAX+1];
    int i;
    printf("digite uma frase:\n");
    for (i=0;i<MAX;i++){
        c = getch();
        if (c =='\r') break;
        vet[i]=c;
        printf("%c",vet[i]);

    }
    vet[MAX]='\0';
    i=0;
    printf("\n");
    while (vet[i]!='\0'){
    printf("%c",vet[i]);
    if (vet[i]==' ') printf("\n");
    i++;
    }
    return 0;
        }
